{
    "config": {
        "cert_type": ["zmq"],
        "source": "gstreamer",
        "source_parameters": {
            "type": "gst",
            "element":"multifilesrc"
        },
        "pipeline": "human_pose",
        "pipeline_version": "human_pose_estimation",
        "publish_frame": true,
        "model_parameters": {}
    },
    "interfaces": {
        "Publishers": [
            {
                "Name": "default",
                "Type": "zmq_tcp",
                "EndPoint": "0.0.0.0:65114",
                "Topics": [
                    "edge_video_analytics_results"
                ],
                "AllowedClients": [
                    "*"
                ]
            }
        ],
        "Subscribers": [
            {
                "Name": "default",
                "Type": "zmq_ipc",
                "EndPoint": "/EII/sockets",
                "PublisherAppName": "VideoIngestion",
                "Topics": [
                    "camera1_stream"
                ],
                "zmq_recv_hwm": 50
            }
        ]
    }
}
